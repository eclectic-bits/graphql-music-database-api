{
  "collectCoverageFrom": ["<rootDir>/src/**/*.{js,jsx,ts,tsx}"],
  "coverageThreshold": {
    "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
    }
  },
  "errorOnDeprecated": true,
  "preset": "ts-jest",
  "roots": ["<rootDir>/tests"],
  "setupFilesAfterEnv": ["<rootDir>/tests/jest.setup.ts"],
  "testEnvironment": "node",
  "verbose": true
}
